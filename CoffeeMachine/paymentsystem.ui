    font-size: 14px;                  /* Smaller font for readability */
    font-weight: bold;                /* Bold font for emphasis */
    color: white;                     /* White text */
    border-radius: 40px;              /* Circular buttons to mimic coins (smaller size) */
    padding: 8px;                     /* Less padding for smaller buttons */
    min-width: 60px;                  /* Smaller width for coin-like buttons */
    min-height: 60px;                 /* Smaller height for coin-like buttons */
    margin: 5px;                      /* Space between buttons */
}

/* 10 Cent Coin - Yellow Color */
QPushButton#tenCent {
    background-color: #FFA500;        /* Orange color for 10c */
    border: 2px solid #FF8C00;        /* Darker orange border */
}

QPushButton#tenCent:hover {
    background-color: #FFB74D;        /* Lighter orange on hover */
    border-color: #FF9800;            /* Lighter border on hover */
}

QPushButton#tenCent:pressed {
    background-color: #FF8C00;        /* Darker orange when pressed */
    border-color: #FF6F00;            /* Even darker border when pressed */
}

/* 20 Cent Coin - Yellow Color */
QPushButton#twentyCent {
    background-color: #FFD700;        /* Gold color for 20c */
    border: 2px solid #FFA500;        /* Darker gold border */
}

QPushButton#twentyCent:hover {
    background-color: #FFC107;        /* Slightly lighter gold on hover */
    border-color: #FF8F00;            /* Darker gold border on hover */
}

QPushButton#twentyCent:pressed {
    background-color: #FFA500;        /* Darker gold when pressed */
    border-color: #FF6F00;            /* Even darker border when pressed */
}

/* 30 Cent Coin - Lighter Yellow */
QPushButton#thirtyCent {
    background-color: #FFEB3B;        /* Bright yellow for 30c */
    border: 2px solid #FBC02D;        /* Slightly darker yellow border */
}

QPushButton#thirtyCent:hover {
    background-color: #FFF176;        /* Lighter yellow on hover */
    border-color: #FDD835;            /* Lighter border on hover */
}

QPushButton#thirtyCent:pressed {
    background-color: #FBC02D;        /* Darker yellow when pressed */
    border-color: #F9A825;            /* Even darker border when pressed */
}

/* 1 Euro Coin - Gold Color */
QPushButton#coinOneEuro {
    background-color: #FFD700;        /* Gold color for €1 */
    border: 2px solid #FFA500;        /* Darker gold border */
}

QPushButton#coinOneEuro:hover {
    background-color: #FFC107;        /* Slightly lighter gold on hover */
    border-color: #FF8F00;            /* Darker gold border on hover */
}

QPushButton#coinOneEuro:pressed {
    background-color: #FFA500;        /* Darker gold when pressed */
    border-color: #FF6F00;            /* Even darker border when pressed */
}

/* 2 Euro Coin - Bi-metallic Look */
QPushButton#coinTwoEuro {
    background-color: qradialgradient(
        cx: 50%, cy: 50%, radius: 50%,
        fx: 50%, fy: 50%,
        stop: 0 #C0C0C0,               /* Silver center */
        stop: 1 #FFD700                 /* Gold outer ring */
    );
    border: 2px solid #A9A9A9;         /* Silver border */
}

QPushButton#coinTwoEuro:hover {
    background-color: qradialgradient(
        cx: 50%, cy: 50%, radius: 50%,
        fx: 50%, fy: 50%,
        stop: 0 #D3D3D3,               /* Lighter silver center on hover */
        stop: 1 #FFC107                 /* Lighter gold outer ring */
    );
    border-color: #A9A9A9;             /* Lighter silver border on hover */
}

QPushButton#coinTwoEuro:pressed {
    background-color: qradialgradient(
        cx: 50%, cy: 50%, radius: 50%,
        fx: 50%, fy: 50%,
        stop: 0 #A9A9A9,               /* Darker silver center when pressed */
        stop: 1 #FFA500                 /* Darker gold outer ring */
    );
    border-color: #808080;             /* Darker silver border when pressed */
}
